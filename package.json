{
	"name": "hotels-menu",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 3005",
		"build": "next build",
		"start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
		"lint": "next lint",
		"prepare": "husky install",
		"check-types": "tsc --pretty --noEmit",
		"check-format": "prettier --check .",
		"check-lint": "eslint . --ext ts --ext tsx --ext js",
		"format": "prettier --config .prettierrc \"**/*.{css,scss,ts,tsx,html,json}\" --write",
		"test-all": "yarn run check-format && yarn run check-lint && yarn run check-types"
	},
	"lint-staged": {
		"*.{ts,tsx}": "yarn check-format"
	},
	"dependencies": {
		"@stripe/stripe-js": "^4.4.0",
		"axios": "^1.7.4",
		"clsx": "^2.1.1",
		"js-cookie": "^3.0.5",
		"jwt-decode": "^4.0.0",
		"next": "14.2.5",
		"next-react-svg": "^1.2.0",
		"qrcode.react": "^4.0.1",
		"react": "18.3.1",
		"react-accessible-accordion": "^5.0.0",
		"react-dom": "18.3.1",
		"react-hook-form": "^7.52.2",
		"react-scroll": "^1.9.0",
		"react-table": "^7.8.0",
		"react-to-print": "^2.15.1",
		"react-toastify": "^10.0.5",
		"react-transition-group": "^4.4.5",
		"sass": "^1.77.8",
		"stripe": "^16.10.0",
		"zustand": "^4.5.5"
	},
	"devDependencies": {
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^18.19.44",
		"@types/react": "^18.3.3",
		"@types/react-scroll": "^1.8.10",
		"@types/react-table": "^7.7.20",
		"@types/react-transition-group": "^4.4.11",
		"@typescript-eslint/eslint-plugin": "^8.1.0",
		"@typescript-eslint/parser": "^8.1.0",
		"eslint": "^8.0.0",
		"eslint-config-next": "14.2.5",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^3.4.1",
		"eslint-plugin-react": "^7.35.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.9",
		"prettier": "^2.0.5",
		"pretty-quick": "^3.3.1",
		"typescript": "^5.5.4"
	}
